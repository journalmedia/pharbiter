{
  "name": "journalmedia/pharbiter",
  "require": {
    "php": ">=7.0",
    "illuminate/container": "~5.2.28",
    "illuminate/support": "~5.2.28",
    "symfony/console": "~3.0",
    "league/flysystem": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.3"
  },
  "autoload": {
    "psr-4": {
      "JournalMedia\\Pharbiter\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/acceptance/AcceptanceTestCase.php"
    ],
    "psr-4": {
      "JournalMedia\\PharbiterTest\\Integration\\": "tests/integration",
      "JournalMedia\\PharbiterTest\\Unit\\": "tests/unit",
      "JournalMedia\\PharbiterTest\\FixtureTests\\": "tests/fixtures/tests"
    }
  }
}
